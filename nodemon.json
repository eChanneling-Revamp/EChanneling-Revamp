{
  "watch": [
    "src",
    "prisma"
  ],
  "ext": "ts,tsx,js,jsx,json",
  "ignore": [
    "src/**/*.test.ts",
    "src/**/*.test.tsx",
    "src/**/*.spec.ts",
    "src/**/*.spec.tsx",
    "node_modules",
    ".next",
    "dist",
    "build"
  ],
  "exec": "next dev",
  "env": {
    "NODE_ENV": "development"
  },
  "delay": 1000,
  "verbose": true,
  "legacyWatch": true,
  "signal": "SIGTERM"
}
